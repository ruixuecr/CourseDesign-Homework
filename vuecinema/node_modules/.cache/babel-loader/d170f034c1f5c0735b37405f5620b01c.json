                    <el-input v-model=\"form.name\" size=\"large\" maxlength=\"20\" clearable placeholder=\"请输入账号/手机号/邮箱\">\n                        <template #prefix>\n                            <el-icon class=\"el-input__icon\"><User /></el-icon>\n                        </template>\n                    </el-input>\n                </el-form-item>\n                <el-form-item label=\"密码\" size=\"large\" prop=\"pass\">\n                    <el-input v-model=\"form.pass\" size=\"large\" maxlength=\"20\" type=\"password\" show-password clearable placeholder=\"请输入密码\">\n                        <template #prefix>\n                            <el-icon class=\"el-input__icon\"><Lock /></el-icon>\n                        </template>\n                    </el-input>\n                </el-form-item>\n                <el-form-item label=\"确认密码\" size=\"large\" prop=\"ck_pass\">\n                    <el-input v-model=\"form.ck_pass\" size=\"large\" maxlength=\"20\" type=\"password\" show-password clearable placeholder=\"请再次输入密码\">\n                        <template #prefix>\n                            <el-icon class=\"el-input__icon\"><CircleCheck /></el-icon>\n                        </template>\n                    </el-input>\n                </el-form-item>\n                <el-form-item>\n                    <slot name=\"cut\"></slot>\n                </el-form-item>\n                <el-form-item>\n                    <el-button type=\"primary\" size=\"large\" style=\"width: 100%\" @click=\"onSubmit('form')\">立即注册</el-button>\n                </el-form-item>\n            </el-form>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { ElMessage } from 'element-plus'\n\nexport default {\n    name: 'Register',\n    data() {\n        return {\n            form: {\n                name: '',\n                pass: '',\n                ck_pass: ''\n            },\n            rules: {\n                name: [\n                    {\n                        required: true,\n                        message: '账号不能为空',\n                        trigger: 'change'\n                    }\n                ],\n                pass: [\n                    {\n                        required: true,\n                        message: '密码不能为空',\n                        trigger: 'change'\n                    }\n                ],\n                ck_pass: [\n                    {\n                        required: true,\n                        validator: this.validatePass2,\n                        trigger: 'change'\n                    }\n                ]\n            }\n        }\n    },\n    methods: {\n        validatePass2(rule, value, callback) {\n            if (value === '') {\n                callback(new Error('确认密码不能为空'))\n            } else if (value !== this.form.pass) {\n                callback(new Error('2次输入的密码不同！请重新输入'))\n            } else {\n                callback()\n            }\n        },\n        onSubmit(formName) {\n            this.$refs[formName].validate((valid) => {\n                if (valid) {\n                    ElMessage({\n                        message: '注册成功！',\n                        type: 'success',\n                        duration: 1000,\n                        onClose: () => {\n                            this.$store.dispatch('movie/revise_login', { name: this.form.name, pass: this.form.pass, remember: false })\n                            this.$emit('success', true)\n                        }\n                    })\n                } else {\n                    return false\n                }\n            })\n        }\n    }\n}\n</script>\n\n<style scoped lang=\"scss\">\n.form-box :deep(.el-form-item__label) {\n    color: var(--el-color-primary);\n}\n\n.form-box :deep(.el-input__wrapper) {\n    background-color: rgba(255, 255, 255, 0.1) !important;\n}\n.form-box :deep(.el-input__inner) {\n    color: var(--el-color-primary);\n}\n\n.three-box {\n    .divider-box {\n        position: relative;\n        text-align: center;\n        overflow: hidden;\n    }\n    .divider-box::before,\n    .divider-box::after {\n        content: '';\n        display: inline-block;\n        width: 100%;\n        height: 1px;\n        position: absolute;\n        background: #ffffff9d;\n        top: 50%;\n    }\n    .divider-box::before {\n        margin-left: -10px;\n        transform: translateX(-100%);\n    }\n    .divider-box::after {\n        margin-left: 10px;\n    }\n}\n</style>\n"],"mappings":";AAyCA,SAASA,SAAQ,QAAS,cAAa;AAEvC,eAAe;EACXC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAI,EAAE;QACFF,IAAI,EAAE,EAAE;QACRG,IAAI,EAAE,EAAE;QACRC,OAAO,EAAE;MACb,CAAC;MACDC,KAAK,EAAE;QACHL,IAAI,EAAE,CACF;UACIM,QAAQ,EAAE,IAAI;UACdC,OAAO,EAAE,QAAQ;UACjBC,OAAO,EAAE;QACb,EACH;QACDL,IAAI,EAAE,CACF;UACIG,QAAQ,EAAE,IAAI;UACdC,OAAO,EAAE,QAAQ;UACjBC,OAAO,EAAE;QACb,EACH;QACDJ,OAAO,EAAE,CACL;UACIE,QAAQ,EAAE,IAAI;UACdG,SAAS,EAAE,IAAI,CAACC,aAAa;UAC7BF,OAAO,EAAE;QACb;MAER;IACJ;EACJ,CAAC;EACDG,OAAO,EAAE;IACLD,aAAaA,CAACE,IAAI,EAAEC,KAAK,EAAEC,QAAQ,EAAE;MACjC,IAAID,KAAI,KAAM,EAAE,EAAE;QACdC,QAAQ,CAAC,IAAIC,KAAK,CAAC,UAAU,CAAC;MAClC,OAAO,IAAIF,KAAI,KAAM,IAAI,CAACX,IAAI,CAACC,IAAI,EAAE;QACjCW,QAAQ,CAAC,IAAIC,KAAK,CAAC,iBAAiB,CAAC;MACzC,OAAO;QACHD,QAAQ,CAAC;MACb;IACJ,CAAC;IACDE,QAAQA,CAACC,QAAQ,EAAE;MACf,IAAI,CAACC,KAAK,CAACD,QAAQ,CAAC,CAACE,QAAQ,CAAEC,KAAK,IAAK;QACrC,IAAIA,KAAK,EAAE;UACPrB,SAAS,CAAC;YACNQ,OAAO,EAAE,OAAO;YAChBc,IAAI,EAAE,SAAS;YACfC,QAAQ,EAAE,IAAI;YACdC,OAAO,EAAEA,CAAA,KAAM;cACX,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,oBAAoB,EAAE;gBAAEzB,IAAI,EAAE,IAAI,CAACE,IAAI,CAACF,IAAI;gBAAEG,IAAI,EAAE,IAAI,CAACD,IAAI,CAACC,IAAI;gBAAEuB,QAAQ,EAAE;cAAM,CAAC;cAC1G,IAAI,CAACC,KAAK,CAAC,SAAS,EAAE,IAAI;YAC9B;UACJ,CAAC;QACL,OAAO;UACH,OAAO,KAAI;QACf;MACJ,CAAC;IACL;EACJ;AACJ","ignoreList":[]}]}