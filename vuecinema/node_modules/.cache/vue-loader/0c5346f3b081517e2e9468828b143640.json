Event(title)\">\r\n                        <span class=\"v-a-middle\">{{ more_title }} </span>\r\n                        <el-icon class=\"v-a-middle\"><ArrowRightBold /></el-icon>\r\n                    </div>\r\n                </slot>\r\n            </div>\r\n        </div>\r\n        <div>\r\n            <slot name=\"content\"></slot>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: 'Content',\r\n    props: ['Title', 'MoreTitle', 'IsShow'],\r\n    watch: {\r\n        Title: {\r\n            handler(newVal) {\r\n                this.title = newVal\r\n            },\r\n            deep: true,\r\n            immediate: true\r\n        },\r\n        MoreTitle: {\r\n            handler(newVal) {\r\n                this.more_title = !newVal ? '更多' : newVal\r\n            },\r\n            deep: true,\r\n            immediate: true\r\n        },\r\n        IsShow: {\r\n            handler(newVal) {\r\n                this.is_show = newVal || newVal != undefined ? newVal : true\r\n            },\r\n            deep: true,\r\n            immediate: true\r\n        }\r\n    },\r\n    data() {\r\n        return {\r\n            title: '热门影视',\r\n            more_title: '更多',\r\n            is_show: true\r\n        }\r\n    },\r\n    methods: {\r\n        clickMoreEvent(data) {\r\n            this.$emit('more', data)\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.demo-tabs {\r\n    width: 100%;\r\n}\r\n</style>\r\n"]}]}